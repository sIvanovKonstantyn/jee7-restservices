<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="WebApp_ID" version="2.5"
xmlns="http://java.sun.com/xml/ns/javaee"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<display-name>RestSevices</display-name>
	<servlet>
		<description>Rest sevices</description>
		<servlet-name>Rest sevices servlet</servlet-name>
		<servlet-class>Yours_AppServer_Servlet_Class</servlet-class> <!--TODO: change to yours-->
		<init-param>
			<param-name>javax.ws.rs.Application</param-name>
			<param-value>demos.jee.restservices.RestApplication</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>Rest sevices servlet</servlet-name>
		<url-pattern>/*</url-pattern>
	</servlet-mapping>
</web-app>